<script type="text/javascript">

function openPdf1()
{
var omyFrame = document.getElementById("myFrame");
omyFrame.style.display="block";
omyFrame.src = "samplereport.pdf";
}
    
function openPdf2()
{
var omyFrame = document.getElementById("myFrame");
omyFrame.style.display="block";
omyFrame.src = "Sample-Pitch-Deck.pdf";
}
